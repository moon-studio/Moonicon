{
  "name": "moon-icon",
  "version": "0.0.1",
  "description": "easy-to-use and rich componentized icon library.",
  "main": "moon-icon/moon-icon.js",
  "type": "module",
  "files": [
    "moon-icon/*"
  ],
  "scripts": {
    "lint": "eslint src --fix --ext .ts,.tsx,.vue",
    "prettier": "prettier --write \"packages/**/*.{vue,ts,tsx}\"",
    "test:vue3": "vitest run packages/moon-icon-vue3",
    "test:util": "vitest run packages/moon-icon-util",
    "test-coverage:vue3": "vitest run --coverage packages/moon-icon-vue3",
    "test-coverage:util": "vitest run --coverage packages/moon-icon-util",
    "example:vue3": "vite packages/moon-icon-vue3/example",
    "website": "vite packages/moon-icon-website",
    "compiler": "ts-node-esm packages/moon-icon-svg/scripts/index.ts",
    "exportJson": "ts-node-esm packages/moon-icon-svg/scripts/exportJson.ts",
    "exportCsv": "json2csv -i packages/moon-icon-svg/resources/index.json -c packages/moon-icon-svg/config/fieldsConfig.json -o packages/moon-icon-website/src/csv/out.csv",
    "build": "npm run compiler && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moon-studio/moon-icon.git"
  },
  "author": "moon-studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moon-studio/moon-icon/issues"
  },
  "homepage": "https://github.com/moon-studio/moon-icon#readme",
  "devDependencies": {
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.1",
    "vite": "^3.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.21.0"
  }
}
